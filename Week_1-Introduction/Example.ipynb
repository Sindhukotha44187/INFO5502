   "source": [
    "### Equation for Finding the Mean\n",
    "\n",
    "$\\mu = \\frac{1}{N} \\sum_{i=1}^{N} x_i$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "45329790",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "\n",
    "def mean(nums):\n",
    "    return np.sum(nums)/len(nums)"
   ]
  }
